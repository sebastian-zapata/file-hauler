<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ImportGroup Label="PropertySheets" />
  <PropertyGroup Label="UserMacros">
    <OutputName>FileSyncer</OutputName>
    <VersionMajor>1</VersionMajor>
    <VersionMinor>0</VersionMinor>
    <VersionPatch>0</VersionPatch>
    <Version>$(VersionMajor).$(VersionMinor).$(VersionPatch)</Version>
    <LocalInstallDir>$(SolutionDir)../installed/$(ProjectName)</LocalInstallDir>
    <GlobalInstallDir>$(FRIENDLYSHADE_INSTALL_PATH)/$(ProjectName)/$(Version)</GlobalInstallDir>
  </PropertyGroup>
  <PropertyGroup />
  <ItemDefinitionGroup />
  <ItemGroup>
    <BuildMacro Include="OutputName">
      <Value>$(OutputName)</Value>
    </BuildMacro>
    <BuildMacro Include="VersionMajor">
      <Value>$(VersionMajor)</Value>
    </BuildMacro>
    <BuildMacro Include="VersionMinor">
      <Value>$(VersionMinor)</Value>
    </BuildMacro>
    <BuildMacro Include="VersionPatch">
      <Value>$(VersionPatch)</Value>
    </BuildMacro>
    <BuildMacro Include="Version">
      <Value>$(Version)</Value>
    </BuildMacro>
    <BuildMacro Include="LocalInstallDir">
      <Value>$(LocalInstallDir)</Value>
    </BuildMacro>
    <BuildMacro Include="GlobalInstallDir">
      <Value>$(GlobalInstallDir)</Value>
    </BuildMacro>
  </ItemGroup>
</Project>